{
  "name": "hackathon-ai-sf-09132025",
  "version": "1.0.0",
  "description": "AIハッカソン（サンフランシスコ、2025年9月13日）のためのプロジェクト",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd src/backend && npm run start",
    "start:frontend": "cd src/frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd src/backend && npm run dev",
    "dev:frontend": "cd src/frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd src/backend && npm run build",
    "build:frontend": "cd src/frontend && npm run build",
    "install:all": "npm install && npm run install:backend && npm run install:frontend",
    "install:backend": "cd src/backend && npm install",
    "install:frontend": "cd src/frontend && npm install"
  },
  "keywords": [
    "hackathon",
    "ai",
    "react",
    "node",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}